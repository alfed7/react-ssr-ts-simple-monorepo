{
  "name": "@rsts/server",
  "version": "0.1.0",
  "main": "build/server-bundle.js",
  "module": "build/esm/server-bundle.esm.js",
  "author": "Alexander Fedorenko",
  "license": "MIT",
  "repository": "https://github.com/alfed7/react-ssr-ts-simple-monorepo.git",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel watch mon",
    "mon": "nodemon build/server-bundle.js",
    "start2": "npm-run-all --parallel watch & nodemon build/server-bundle.js",
    "prestart": "yarn workspace @rsts/web build",
    "tsc": "tsc -b"
  },
  "dependencies": {
    "@rsts/web": "0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "express": "^4.17.1",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "typescript": "^4.5.2",
    "nodemon": "^2.0.15"
  }
}
